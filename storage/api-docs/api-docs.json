                    "required": true,
                    "content": {
                        "application/json": {
                            "schema": {
                                "required": [
                                    "username",
                                    "password"
                                ],
                                "properties": {
                                    "username": {
                                        "type": "string",
                                        "example": "Nugrah"
                                    },
                                    "password": {
                                        "type": "string",
                                        "format": "password",
                                        "example": "rahasia"
                                    },
                                    "name": {
                                        "type": "string",
                                        "example": "Fabianugerah Bainashhiddiq"
